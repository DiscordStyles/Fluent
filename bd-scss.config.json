{
	"meta": {
		"name": "Fluent",
		"author": "Gibbu#1211",
		"version": "1.0.5",
		"description": "Brings the look of Windows 11 to Discord.",
		"source": "https://github.com/DiscordStyles/Fluent",
		"invite": "ZHthyCw"
	},
	"compiler": {
		"prefix": true
	},
	"dev": {
		"target": ["src", "dev.scss"],
		"output": ["Fluent.theme.css"]
	},
	"dist": {
		"target": ["src", "dist.scss"],
		"output": ["dist", "Fluent.theme.css"]
	},
	"base": {
		"target": ["src", "base.scss"],
		"output": ["dist", "Fluent.css"]
	},
	"addons": [
		{
			"target": ["src", "addons", "_selfmessages.scss"],
			"output": ["dist", "SelfMessages.css"]
		}
	]
}